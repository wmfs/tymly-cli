{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "description": "Snacks - crisps, peanuts, pork scratchings, etc",
  "type": "object",
  "properties": {
    "code": {
      "type": "string"
    },
    "label": {
      "title": "Customer-facing label",
      "type": "string"
    },
    "gluten-free": {
      "title": "Suitable for celiacs?",
      "type": "boolean"
    },
    "contains-nuts": {
      "title": "Got some nuts?",
      "type": "boolean"
    }
  },
  "required": [
    "code",
    "label"
  ],
  "primaryKey": [
    "code"
  ]
}
